    "@types/speakeasy": "^2.0.5",
    "bcrypt": "^5.0.0",
    "crypto-random-string": "^3.3.1",
    "email-validator": "^2.0.4",
    "file-type": "^15.0.1",
    "gcp-metadata": "^4.2.1",
    "google-auth-library": "^6.1.6",
    "mariadb": "^2.5.3",
    "moment": "^2.29.4",
    "moment-range": "^4.0.2",
    "node-schedule": "^1.3.3",
    "nodemailer": "^6.4.18",
    "qrcode": "^1.4.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
